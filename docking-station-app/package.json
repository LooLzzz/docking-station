{
  "name": "docking-station",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "cd ./src/app && fastapi dev api/main.py --reload --host 0.0.0.0 --port ${SERVER_PORT:-3001}",
    "next-dev": "next dev -H 0.0.0.0 -p ${WEB_PORT:-3000}",
    "dev": "concurrently --names=WEB,SERVER --prefix-colors=auto \"bun run next-dev\" \"bun run fastapi-dev\"",
    "build": "next build",
    "start": "next start -p ${WEB_PORT:-3000}",
    "lint": "next lint"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.8",
    "@mantine/core": "^7.5.1",
    "@mantine/form": "^7.5.1",
    "@mantine/hooks": "^7.5.1",
    "@mantine/modals": "^7.5.1",
    "@mantine/notifications": "^7.5.1",
    "@tabler/icons-react": "^2.47.0",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.6.7",
    "immer": "^10.1.1",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-use-websocket": "^4.8.1",
    "sass": "^1.79.2",
    "sharp": "^0.33.4",
    "zustand": "^4.5.0",
    "zustand-computed": "^1.4.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8.4.33",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5"
  }
}
